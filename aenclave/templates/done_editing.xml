{% load aenclave %}
<contents>
 <cell name="track">{{song.track_string}}</cell>
 <cell name="title"{% if not no_queuing %} class="{% if song.last_queued|is_recent:1 %}recent{% else %}queue{% endif %}" link="/audio/queue?ids={{song.id}}"{% endif %}>{{song.title|escape}}</cell>
 <cell name="album" link="/audio/albums/{{song.album|escape}}/">{{song.album|escape}}</cell>
 <cell name="artist" link="/audio/artists/{{song.artist|escape}}/">{{song.artist|escape}}</cell>
</contents>
